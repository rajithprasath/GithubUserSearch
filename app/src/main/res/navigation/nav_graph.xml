    android:id="@+id/nav_graph"
    app:startDestination="@id/searchUserFragment">

    <fragment
        android:id="@+id/searchUserFragment"
        android:name="com.rajith.payconiq.home.search.view.fragment.SearchUserFragment"
        android:label="SearchUserFragment" >
        <action
            android:id="@+id/action_searchUserFragment_to_viewDetailFragment"
            app:destination="@id/viewDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/viewDetailFragment"
        android:name="com.rajith.payconiq.home.detail.view.fragment.UserDetailFragment"
        android:label="ViewDetailFragment">
    <argument
        android:name="login"
        app:argType="string"
        android:defaultValue="" />

    </fragment>
</navigation>